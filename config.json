  },
  "building": {
    "manage_buildings": true,
    "default": "purple_predator",
    "max_lookahead": 2,
    "max_queued_items": 2
  },
  "units": {
    "recruit": true,
    "upgrade": true,
    "default": "basic",
    "batch_size": 25,
    "manage_defence": true,
    "remove_manual_queued": false,
    "randomize_unit_queue": true
  },
