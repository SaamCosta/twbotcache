    "name": "tw_bot",
    "version": "2.1",
    "author": "stefan2200"
  },
  "server": {
    "server": "br135",
    "endpoint": "https://br135.tribalwars.com.br/game.php",
    "server_on_twstats": true
  },
  "reporting": {
    "enabled": false,
    "connection_string": "file://cache/logs/twb_{ts}.log"
  },
